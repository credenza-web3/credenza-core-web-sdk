{
	"extends": ["./.svelte-kit/tsconfig.json", "tsconfig/tsconfig.json"],
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true
	},
	"include": ["src", "vite.config.ts", "tests"]
}
